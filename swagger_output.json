{
  "openapi": "3.0.0",
  "info": {
  "title": "API de usuarios y Servicios",
  "description": "API para gestionar usuarios y servicios, usando metodos crud, con autenticacion JWT y encriptacion de contrase√±as con bcryptjs y base de datos MongoDB",
  "version": "1.0.0"
  },
  "host": "https://backend-autentificacion.onrender.com",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/api/v1/users/sigin": {
      "post": {
        "description": "Crea un nuevo usuario",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "any-name"
                },
                "mail": {
                  "example": "any@gmail.com"
                },
                "description": {
                  "example": "any"
                },
                "password": {
                  "example": "any123"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/v1/users/login": {
      "post": {
        "description": "logea un usuario",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "mail": {
                  "example": "any@gmail.com"
                },
                "password": {
                  "example": "any123"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/api/v1/users/loginVerification": {
      "get": {
        "description": "Verifica el token JWT",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/v1/users/{id}": {
      "put": {
        "description": "Modifica un usuario por ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "any-name"
                },
                "mail": {
                  "example": "any@gmail.com"
                },
                "description": {
                  "example": "any"
                },
                "password": {
                  "example": "any123"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/v1/services/": {
      "get": {
        "description": "obtiene todos los servicios.",
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "No se pudo obtener los servicios"
          }
        }
      },
      "/api/v1/services/": {
      "post": {
        "description": "Crea un nuevo servicio.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "any-name-service"
                },
                "price": {
                  "example": "any-price-$$$$"
                },
                "description": {
                  "example": "any-description-blablabla"
                 }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK."
          },
          "404": {
            "description": "Not Found."
          },
          "500": {
            "description": "No se pudo crear el servicio."
          }
        }
      }
    },
    "/api/v1/services/{id}": {
      "put": {
        "description": "Modifica un servicio por ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "any-name-service"
                },
                "price": {
                  "example": "any-price-$$$$$"
                },
                "description": {
                  "example": "any-description-blablabla"
                }
              }
            }
          }
        ],
          "responses": {
          "200": {
            "description": "OK."
          },
          "404": {
            "description": "Not Found."
          },
          "500": {
            "description": "No se pudo modificar el servicio."
          }
        }
      },
      "delete": {
        "description": "Elimina un servicio por ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK."
          },
          "404": {
            "description": "Not Found."
          },
          "500": {
            "description": "No se pudo encontrar la id del servicio a eliminar."
        }
      },
      "get": {
        "description": "Obtiene un servicio por ID",
        "responses": {
          "200": {
            "description": "OK."
          },
          "404": {
            "description": "Not Found."
          },
          "500": {
            "description": "No se encontro la id del servicio."
          }
        }
      }
    }
  }
}
}
}